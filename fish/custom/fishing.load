function ll
  ls -lG
end

function cls
  clear
end

function herro
  fish ~/.osx-setup/me.fish
end

function battery
  pmset -g ps | sed -e '$!N;s/\n/ /' -e 's/\;\ AC\ attached\;\ not\ charging//;s/discharging\;//' -e 's/Currenty\ drawing\ from/\ /;s/\ \ -Internal/\ \ \|\ \ /;s/-0./\ :\ \ /;s/\;/\ \ \ \>\ /g' | iconv -f utf-8 -t ucs-2-internal
end
